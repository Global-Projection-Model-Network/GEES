!variables
    a
    y, c, k, n, i
    puk, w, r, v, pk
    py
    pq, q, qq, y2

!shocks
    shk

!log-variables
    !all-but

!equations


    v * (c-0.5*c{-1}) = 1;
    v = 0.95 * v{+1} * r;

    v * w = 1;

    k = (1-0.15)*k{-1} + i;
    pk = puk + 0.95*v{+1}/v*(1-0.15)*pk{+1};
    pk = 1;

    y2 = c + i;

    y = k^0.4 * (a*n)^0.6;
    0.4 * py * y = puk * k;
    0.6 * py * y = w * n;

    y2 = y^0.8 * q^0.2;
    0.8 * y2 = py * y;
    0.2 * y2 = pq * q;

    qq = a * &q/&a !! pq = 1;
    pq = (q/qq)^0.5 !! qq = q;

    log(a) = log(a{-1}) + shk;

